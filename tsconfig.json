{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["ES2017", "DOM", "DOM.Iterable"],
    "jsx": "react",

    "module": "ES2022",
    "moduleResolution": "node",
    "resolveJsonModule": true,

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,

    "skipLibCheck": true
  }
}
